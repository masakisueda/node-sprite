@media (min--moz-device-pixel-ratio: 1),
(-o-min-device-pixel-ratio: 1),
(-webkit-min-device-pixel-ratio: 1),
(min-device-pixel-ratio: 1) {
_sprite {
  background-image: url( 'output-1.png' );
  background-repeat: no-repeat;
  background-size: NaNpx NaNpx;
}
}


@media (min--moz-device-pixel-ratio: 1.3),
(-o-min-device-pixel-ratio: 1.3),
(-webkit-min-device-pixel-ratio: 1.3),
(min-device-pixel-ratio: 1.3) {
_sprite {
  background-image: url( 'output-1.3.png' );
  background-repeat: no-repeat;
  background-size: NaNpx NaNpx;
}
}


@media (min--moz-device-pixel-ratio: 1.5),
(-o-min-device-pixel-ratio: 1.5),
(-webkit-min-device-pixel-ratio: 1.5),
(min-device-pixel-ratio: 1.5) {
_sprite {
  background-image: url( 'output-1.5.png' );
  background-repeat: no-repeat;
  background-size: NaNpx NaNpx;
}
}


@media (min--moz-device-pixel-ratio: 2),
(-o-min-device-pixel-ratio: 2),
(-webkit-min-device-pixel-ratio: 2),
(min-device-pixel-ratio: 2) {
_sprite {
  background-image: url( 'output-2.png' );
  background-repeat: no-repeat;
  background-size: NaNpx NaNpx;
}
}

_sprite.100x200 {
  width: 50px;
  height: 100px;
  background-position: 0 0;
}
_sprite.200x100 {
  width: 100px;
  height: 50px;
  background-position: -50px 0;
}
_sprite.100x100 {
  width: 50px;
  height: 50px;
  background-position: -50px -50px;
}
_sprite.100x100-2 {
  width: 50px;
  height: 50px;
  background-position: -100px -50px;
}
